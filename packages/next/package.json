{
  "name": "@sujii/rsc-with-go-graphql-next",
  "version": "0.0.1",
  "description": "ReacrServerComponents-Next-APP",
  "scripts": {
    "setup": "npm-run-all -p setup:**",
    "setup:test": "cross-env BUILD_ENV=\"test\" cp dotenv.test .env.test",
    "setup:development": "cross-env BUILD_ENV=\"development\" cp dotenv.development .env.development",
    "dev": "next dev",
    "build": "next build",
    "build:dev": "cross-env BUILD_ENV=\"development\" next build",
    "start": "next start",
    "test": "jest --watchAll",
    "lint": "eslint ./ --ext .js,jsx,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,jsx,.ts,.tsx --fix",
    "prettier": "prettier --check --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "prettier:fix": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "generate": "graphql-codegen --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "^4.28.0",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.40",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "next": "13.4.10",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "react-icons": "^4.8.0",
    "recoil": "^0.7.7",
    "recoil-persist": "^5.1.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "eslint-config-prettier": "^8.7.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "sass": "^1.60.0",
    "ts-node": "^10.9.1"
  },
  "private": true
}
